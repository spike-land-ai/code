    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/mime-types": "3.0.1",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/react-syntax-highlighter": "15.5.13",
    "@types/recordrtc": "5.6.15",
    "@vitejs/plugin-react-swc": "4.2.3",
