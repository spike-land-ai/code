{
  "name": "@spike-land-ai/code",
  "version": "0.9.57",
  "type": "module",
  "description": "Frontend React application for the spike.land real-time collaborative code editor, featuring Monaco Editor, TypeScript, and live preview.",
  "entry": "dist/modules.mjs",
  "main": "dist/modules.mjs",
  "source": "src/modules.ts",
  "types": "dts/src/modules.d.ts",
  "module": "dist/modules.mjs",
  "files": [
    "dist",
    "src",
    "dts"
  ],
  "scripts": {
    "dev": "concurrently --kill-others-on-fail=false \"npm run dev:vite\" \"npm run types:watch\" \"npm run dev:workers\"",
    "dev:vite": "vite",
    "dev:workers": "tsx src/@/lib/esbuild-dev.ts --watch",
    "build": "npm run clean && npm run build:all",
    "build:all": "concurrently --kill-others-on-fail \"npm run build:vite\" \"npm run build:types\" \"npm run build:workers\" && npm run post-build",
    "build:vite": "vite build",
    "build:vite:watch": "vite build --watch --outDir dist & npm run types:build",
    "build:workers": "tsx src/@/lib/esbuild-dev.ts",
    "build:types": "tsc --build && npm run types:cp",
    "post-build-delete": "npm run remove-extraneous",
    "post-build": "cp -a -f dist-vite/. dist/  && npm run remove-extraneous",
    "remove-extraneous": "cd dist && (rm -f *.tsbuildinfo 2>/dev/null || true) && cd @/workers/monaco && (rm -f css.worker.js html.worker.js json.worker.js 2>/dev/null || true)",
    "types:check": "tsc --noEmit",
    "types:build": "tsc --build",
    "types:watch": "tsc --watch",
    "types:cp": "cp -r dts/src/reactMod.d.ts dts/src/reactDom.d.ts dts/src/jsx.d.ts dts/src/emotion.d.ts dts/src/reactDomClient.d.ts dist/ && cp -r dts/src/@ dist/.",
    "test": "sh scripts/run-tests.sh",
    "test:full": "npm run types:check && npm run lint && npm run test",
    "clean": "rm -rf .tsBuildInfo dist dts dist-vite && mkdir -p dist dts dist-vite",
    "lint": "eslint .",
    "fmt": "dprint fmt \"**/*.{ts,tsx,js,jsx,json,mjs}\"",
    "preview": "vite preview"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "typescript",
    "html",
    "vscode"
  ],
  "authors": [
    "Zoltan Erdos <zolika84@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/spike-land-ai/code"
  },
  "homepage": "http://spike.land",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spike-land-ai/code.git"
  },
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@dmsnell/diff-match-patch": "1.1.0",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@heroicons/react": "2.2.0",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-tooltip": "1.2.8",
    "@spike-land-ai/esbuild-wasm": "0.27.3",
    "@tailwindcss/browser": "4.2.1",
